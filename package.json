{
  "name": "next-with-auth-starter",
  "version": "1.0.0",
  "description": "Starter for Next.js projects with authentication",
  "main": "index.js",
  "scripts": {
    "dev": "next -p 8080",
    "build": "next build",
    "start": "next start -p 8080",
    "export": "next build && next export",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "next",
    "nextjs",
    "authentication"
  ],
  "author": "victorocna <victor@chesscoders.com> (https://chesscoders.com)",
  "dependencies": {
    "@tailwindcss/custom-forms": "^0.2.1",
    "autoprefixer": "^9.8.6",
    "dotenv": "^8.2.0",
    "formik": "^2.1.5",
    "js-cookie": "^2.2.1",
    "jsonwebtoken": "^8.5.1",
    "next": "^9.5.1",
    "node-fetch": "^2.6.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-query": "^1.5.10",
    "react-spring": "^8.0.27",
    "tailwindcss": "^1.6.0",
    "toasted-notes": "^3.2.0",
    "yup": "^0.29.2"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "eslint": "^7.6.0",
    "eslint-plugin-react": "^7.20.5",
    "prettier": "^2.0.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "node": true,
      "commonjs": true,
      "es6": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:react/recommended"
    ],
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "rules": {
      "no-warning-comments": "warn",
      "react/react-in-jsx-scope": "off",
      "react/prop-types": "off"
    }
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "arrowParens": "always"
  }
}
